          <JSR223Assertion guiclass="TestBeanGUI" testclass="JSR223Assertion" testname="JSR223 Assertion" enabled="true">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">def responseTimes = vars.getObject(&quot;responseTimes&quot;) ?: [:]
def groupName = vars.get(&quot;GroupName&quot;)
def responseTime = prev.getTime()

if (!responseTimes.containsKey(groupName)) {
    responseTimes[groupName] = []
}

responseTimes[groupName] &lt;&lt; responseTime
vars.putObject(&quot;responseTimes&quot;, responseTimes)

def maxDeviation = 100 // Max acceptable deviation in ms
responseTimes.each { group, times -&gt;
    def avg = times.sum() / times.size()
    times.each { time -&gt;
        if (Math.abs(time - avg) &gt; maxDeviation) {
            AssertionResult.setFailure(true)
            AssertionResult.setFailureMessage(&quot;Query group $group exceeds deviation limits.&quot;)
